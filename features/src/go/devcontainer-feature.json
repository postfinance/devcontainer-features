{
    "id": "go",
    "version": "0.1.0",
    "name": "Go",
    "description": "A feature which installs Go.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "1.24",
                "1.21.8"
            ],
            "default": "latest",
            "description": "The version of Go to install."
        },
        "versionResolve":{
            "type":"boolean",
            "default": false,
            "description": "Whether to resolve the version automatically."
        },
        "downloadUrlBase": {
            "type": "string",
            "default": "",
            "proposals": [
                "https://mycompany.com/artifactory/dl-google-generic-remote"
            ],
            "description": "The download URL to use for Go binaries."
        },
        "downloadUrlPath": {
            "type": "string",
            "default": "",
            "description": "The download URL path to use for Go binaries."
        },
        "latestUrl": {
            "type": "string",
            "default": "",
            "description": "The URL to fetch the latest Go version from."
        },
        "versionsUrl": {
            "type": "string",
            "default": "",
            "description": "The URL to fetch the available Go versions from."
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go"
            ]
        }
    },
    "containerEnv": {
        "PATH": "/usr/local/go/bin:${PATH}"
    }
}