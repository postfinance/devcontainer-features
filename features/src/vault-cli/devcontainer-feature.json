{
    "id": "vault-cli",
    "version": "0.2.0",
    "name": "Vault CLI",
    "description": "Installs the Vault CLI.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "1.18.2",
                "1"
            ],
            "default": "latest",
            "description": "The version of Vault CLI to install."
        },
        "downloadUrl": {
            "type": "string",
            "default": "",
            "proposals": [
                "https://mycompany.com/artifactory/hashicorp-releases-generic-remote/vault"
            ],
            "description": "The download URL to use for Vault CLI binaries."
        },
        "versionsUrl": {
            "type": "string",
            "default": "",
            "description": "The URL to fetch the available Vault CLI versions from."
        }
    }
}
